<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tutran.aaogpa.data.models.Course" table="COURSE">
        <id name="id" type="int" column="id">
            <generator class="native" />
        </id>
        <property name="courseId" column="co_id" type="string" />
        <property name="name" column="co_name" type="string"/>
        <property name="credit" column="co_credit" type="int" />
    </class>
</hibernate-mapping>